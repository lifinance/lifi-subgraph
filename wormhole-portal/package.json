{
  "name": "wormhole-portal",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ 0xksure/wormhole-portal",
    "prepare:eth": "mustache config/eth.prod.json subgraph.template.yaml > subgraph.yaml",
    "deploy:eth": "yarn prepare:eth && graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ 0xksure/wormhole-portal",
    "create-local": "graph create --node http://localhost:8020/ 0xksure/wormhole-portal",
    "remove-local": "graph remove --node http://localhost:8020/ 0xksure/wormhole-portal",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 0xksure/wormhole-portal",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.34.0",
    "@graphprotocol/graph-ts": "0.28.0"
  },
  "devDependencies": {
    "matchstick-as": "0.5.0",
    "mustache": "^4.2.0"
  }
}
